{
  "name": "codelens-sh",
  "displayName": "CodeLens for Shell Scripts",
  "description": "VS Code's CodeLens for Shell Scripts",
  "version": "1.11.0",
  "publisher": "codelens-sh",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hungtran0203/codelens-sh"
  },
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "title": "Enable CodeLens",
        "command": "codelens-sh.enableCodeLens",
        "category": "CodeLens Sh"
      },
      {
        "title": "Disable Codelens",
        "command": "codelens-sh.disableCodeLens",
        "category": "CodeLens Sh"
      }
    ],
    "configuration": {
      "properties": {
        "codelens-sh.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint",
    "watch": "tsc -watch -p ./",
    "deploy": "npm version minor --no-git-tag-version && git add package.json && git commit -m \"chore: bump version to $(jq -r .version package.json)\" && git tag v$(jq -r .version package.json) && git push --follow-tags"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.11.0"
  }
}
